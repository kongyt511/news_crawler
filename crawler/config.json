{
  "sources": [
    {
      "name": "sina",
      "root_urls": [
        "https://news.sina.com.cn",
        "https://finance.sina.com.cn"
      ],
      "allow_domains": ["finance.sina.com.cn"],
      "blank_list_patterns": [],
      "news_path_patterns": ["\\/doc-.*\\.shtml$"],
      "interval": {"min": 2, "max": 5}
    },
    {
      "name": "eastmoney",
      "root_urls": [
        "https://finance.eastmoney.com"
      ],
      "allow_domains": ["eastmoney.com"],
      "blank_list_patterns": [],
      "news_path_patterns": ["/a/\\d+\\.html$"],
      "interval": {"min": 1, "max": 3}
    },
    {
      "name": "sohu",
      "root_urls": [
        "https://business.sohu.com"
      ],
      "allow_domains": ["sohu.com"],
      "blank_list_patterns": ["stock\\.sohu\\.com"],
      "news_path_patterns": ["/a/"],
      "interval": {"min": 1, "max": 3}
    },
    {
      "name": "news163",
      "root_urls": [
        "https://money.163.com"
      ],
      "allow_domains": ["163.com"],
      "blank_list_patterns": [],
      "news_path_patterns": ["/article/.*\\.html$"],
      "interval": {"min": 1, "max": 3}
    },
    {
      "name": "ifeng",
      "root_urls": [
        "https://finance.ifeng.com/"
      ],
      "allow_domains": ["ifeng.com"],
      "blank_list_patterns": [],
      "news_path_patterns": ["/c/[a-zA-Z0-9]+$"],
      "interval": {"min": 1, "max": 3}
    }
  ]
}
